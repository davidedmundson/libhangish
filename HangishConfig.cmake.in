# set the directories
if(NOT HANGISH_INSTALL_DIR)
   set(HANGISH_INSTALL_DIR "@CMAKE_INSTALL_PREFIX@")
endif(NOT HANGISH_INSTALL_DIR)

set(HANGISH_INCLUDE_DIR              "${HANGISH_INSTALL_DIR}/@HANGISH_INCLUDE_DIR@")
set(HANGISH_LIB_DIR                  "${HANGISH_INSTALL_DIR}/@HANGISH_LIB_DIR@")

set(HANGISH_LIBRARIES -L${HANGISH_LIB_DIR} -lhangish)
